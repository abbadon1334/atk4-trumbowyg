{
  "name": "abbadon1334/atk4-trumbowyg",
  "description": "ATK4 WYSIWYG editor using javascript library trumbowyg",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Francesco Danti",
      "email": "fdanti@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "atk4/ui": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "<6",
    "phpunit/phpcov": "^3.0",
    "codeclimate/php-test-reporter": "*",
    "behat/behat": "*",
    "behat/mink-selenium2-driver": "^1.3",
    "behat/mink-extension": "^2.2",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/process": "^4.3",
    "leanphp/behat-code-coverage": "^3.2@dev"
  },
  "autoload": {
    "psr-4": {
      "atk4\\ui\\FormField\\":"src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TextEditor\\Tests\\":"tests/"
    }
  }
}
